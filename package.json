{
  "name": "override-workspace",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo: liboverlay (generic overlay library) + clash-overlay (Clash/Mihomo config)",
  "type": "module",
  "scripts": {
    "test": "pnpm -r test",
    "test:watch": "pnpm -r --parallel test:watch",
    "typecheck": "pnpm -r typecheck",
    "build": "pnpm --filter clash-overlay build",
    "clean": "pnpm -r clean"
  },
  "devDependencies": {
    "esbuild": "^0.25.0",
    "typescript": "^5.8.0",
    "vitest": "^4.0.18"
  },
  "license": "MIT"
}
